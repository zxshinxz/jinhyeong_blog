{
  "homepage": "jeon-jinhyeong.github.io/jinhyeong_blog",
  "name": "jinhyeong_blog",
  "version": "0.2.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "11.1.2",
    "@emotion/styled": "11.0.0",
    "@material-ui/core": "4.11.2",
    "@material-ui/icons": "4.11.2",
    "@sentry/react": "5.30.0",
    "@sentry/tracing": "5.30.0",
    "@storybook/react": "6.1.11",
    "@testing-library/jest-dom": "5.11.4",
    "@testing-library/react": "11.1.0",
    "@testing-library/user-event": "12.1.10",
    "@types/d3": "6.2.0",
    "@types/d3-axis": "2.0.0",
    "@types/d3-selection": "2.0.0",
    "@types/faker": "5.1.6",
    "@types/jest": "26.0.15",
    "@types/lodash": "4.14.166",
    "@types/node": "12.0.0",
    "@types/react": "16.9.53",
    "@types/react-dom": "16.9.8",
    "@types/react-scroll": "1.8.2",
    "@types/react-sticky-el": "1.0.2",
    "@types/react-virtualized": "9.21.10",
    "axios": "0.21.1",
    "d3": "6.3.1",
    "faker": "5.2.0",
    "lodash": "4.17.20",
    "material-auto-rotating-carousel": "3.0.2",
    "material-ui-dots": "2.0.2",
    "moment": "2.29.1",
    "react": "17.0.1",
    "react-base-table": "1.12.0",
    "react-dom": "17.0.1",
    "react-intersection-observer": "8.31.0",
    "react-query": "3.8.2",
    "react-responsive-carousel": "3.2.11",
    "react-scroll": "1.8.1",
    "react-slick": "0.27.13",
    "react-sticky-el": "2.0.5",
    "react-swipeable-views": "0.13.9",
    "react-virtualized": "9.22.3",
    "react-waypoint": "9.0.3",
    "react-router-dom": "5.2.0",
    "web-vitals": "0.2.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "eslintConfig": {
    "plugins": [
      "@typescript-eslint",
      "react-hooks",
      "prettier"
    ],
    "extends": [
      "airbnb",
      "plugin:react/recommended",
      "plugin:jsx-a11y/recommended",
      "plugin:import/errors",
      "plugin:import/warnings",
      "plugin:import/typescript",
      "plugin:@typescript-eslint/recommended",
      "plugin:prettier/recommended",
      "prettier",
      "prettier/react"
    ],
    "env": {
      "es6": true,
      "node": true
    },
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "project": "./tsconfig.json"
    },
    "rules": {
      "no-use-before-define": "off",
      "prettier/prettier": "error",
      "import/extensions": [
        "error",
        "ignorePackages",
        {
          "js": "never",
          "jsx": "never",
          "ts": "never",
          "tsx": "never"
        }
      ],
      "import/no-extraneous-dependencies": [
        "error",
        {
          "devDependencies": [
            "**/*.stories.tsx"
          ]
        }
      ],
      "react/jsx-filename-extension": [
        1,
        {
          "extensions": [
            ".js",
            ".jsx",
            ".ts",
            ".tsx"
          ]
        }
      ],
      "@typescript-eslint/no-use-before-define": [
        "error"
      ],
      "react/no-unused-prop-types": "off",
      "react/jsx-key": "off",
      "jsx-a11y/click-events-have-key-events": "off",
      "jsx-a11y/no-static-element-interactions": "off",
      "import/prefer-default-export": "off",
      "@typescript-eslint/ban-types": "off",
      "@typescript-eslint/ban-ts-comment": "off",
      "@typescript-eslint/no-this-alias": "off",
      "react/no-this-in-sfc": "off",
      "react/destructuring-assignment": "off",
      "camelcase": "off"
    },
    "settings": {
      "import/resolver": {
        "node": {
          "extensions": [
            ".js",
            ".jsx",
            ".ts",
            ".tsx"
          ]
        }
      }
    }
  },
  "eslintIgnore": [
    "templates/component/*"
  ],
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@storybook/addon-actions": "6.1.11",
    "@storybook/addon-essentials": "6.1.11",
    "@storybook/addon-knobs": "6.1.16",
    "@storybook/addon-links": "6.1.11",
    "@storybook/addons": "6.1.17",
    "@storybook/node-logger": "6.1.11",
    "@storybook/preset-create-react-app": "3.1.5",
    "@storybook/theming": "6.1.17",
    "@types/react-slick": "0.23.4",
    "@typescript-eslint/eslint-plugin": "4.14.1",
    "@typescript-eslint/parser": "4.14.1",
    "babel-loader": "8.1.0",
    "eslint": "7.18.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-airbnb-typescript": "12.0.0",
    "eslint-config-prettier": "7.2.0",
    "eslint-config-react-app": "6.0.0",
    "eslint-import-resolver-typescript": "2.3.0",
    "eslint-loader": "4.0.2",
    "eslint-plugin-flowtype": "5.2.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "3.3.1",
    "eslint-plugin-react": "7.22.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "prettier": "2.2.1",
    "prettier-eslint": "12.0.0",
    "prettier-eslint-cli": "5.0.0",
    "react-scripts": "4.0.0-next.117",
    "@types/react-router-dom": "5.1.7",
    "typescript": "4.0.3"
  }
}
